<!DOCTYPE html>
<html>
  <%- include("./partials/head") %>
  <body>
    <%- include("./partials/navbar") %>

    <h3>Here are a list of users:</h3>

    <% users.forEach((user) => { %>
    <div class="albumCard">
      <p>User name: <%= user.username %></p>
      <a href="/users/<%=user._id%>">More info about this user</a>
    </div>
    <% }) %>
  </body>
</html>
